{
    "name": "lucien-website",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build --mode production",
        "preview": "vite preview",
        "prepare": "husky install",
        "check-updates": "pnpm dlx npm-check-updates",
        "apply-updates": "pnpm dlx npm-check-updates -u && pnpm install",
        "start:server": "pnpm exec nodemon server.js"
    },
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^1.0.5",
        "@tsconfig/svelte": "^3.0.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "nodemon": "^2.0.19",
        "prettier": "^2.7.1",
        "prettier-plugin-svelte": "^2.7.0",
        "svelte": "^3.50.1",
        "typescript": "^4.8.3",
        "vite": "^3.1.0"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown --plugin-search-dir=. ./**/*.html"
    },
    "dependencies": {
        "dotenv": "^16.0.2",
        "express": "^4.18.1",
        "svelte-navigator": "^3.2.2",
        "uuid": "^9.0.0"
    }
}
