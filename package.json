{
    "name": "lucien-website",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "pnpm exec vite",
        "build": "pnpm exec vite build --mode production",
        "preview": "pnpm exec vite preview",
        "format": "pnpm exec prettier --config ./.prettierrc 'src/**/*.ts' --write",
        "check": "pnpm exec svelte-check --tsconfig ./tsconfig.json",
        "prepare": "husky install",
        "check-updates": "pnpm dlx npm-check-updates",
        "apply-updates": "pnpm dlx npm-check-updates -u && pnpm install",
        "start:server": "pnpm exec nodemon server.js"
    },
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^1.0.6",
        "@tsconfig/svelte": "^3.0.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "nodemon": "^2.0.20",
        "prettier": "^2.7.1",
        "prettier-plugin-svelte": "^2.7.0",
        "sass": "^1.54.9",
        "svelte": "^3.50.1",
        "svelte-check": "^2.9.0",
        "svelte-preprocess": "^4.10.7",
        "tslib": "^2.4.0",
        "typescript": "^4.8.3",
        "vite": "^3.1.2"
    },
    "lint-staged": {
        "**/*": "pnpm exec prettier --write --ignore-unknown --plugin-search-dir=. ./**/*.html"
    },
    "dependencies": {
        "dotenv": "^16.0.2",
        "express": "^4.18.1",
        "svelte-navigator": "^3.2.2"
    }
}
